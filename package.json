  "scripts": {
    "clean": "rm -rf out/debug",
    "clean-release": "rm -rf out/release",
    "prebuild": "npm run clean",
    "build": "webpack -d",
    "postbuild": "cp ./res/* out/debug",
    "prebuild-release": "npm run clean-release",
